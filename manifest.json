{
	"manifest_version": 2,

	"name": "SimplePick",
	"description": "doing nothing",
	"version": "0.1",
	"author": "VutonDesign / Smrman",
	"icons": {
			"16":	"icon16.png",
			"32": 	"icon32.png",
			"48": 	"icon48.png",
			"64": 	"icon64.png",
			"128":	"icon128.png"
	},
  
	"browser_action": {
		"default_icon": "icon.png",
		"default_title": "SimplePick - doing nothing"
