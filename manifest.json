{
  "manifest_version": 2,

  "name": "Praintab",
  "description": "A chrome extension that saves bookmark space.",
  "version": "0.0.1",
  "default_locale": "en",
  "author": "AravNarula/Sean",
  "icons": {
    "16": "/img/icon16.png",
    "32": "/img/icon32.png",
    "48": "/img/icon48.png",
    "64": "/img/icon64.png",
    "128": "/img/icon128.png"
  },

  "browser_action": {
    "default_icon": "/img/icon64.png",
    "default_title": "PrainTab - A chrome extension that saves bookmark space.",
    "default_popup": "popup.html"
  },

  "permissions": ["activeTab", "tabs", "newtab"],

  "background": {
    "scripts": ["js/main.js"]
  },
    "chrome_url_overrides": {
    "newtab": "newtab.html"
  }
}
