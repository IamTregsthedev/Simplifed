{
  "manifest_version": 2,

  "name": "Praintab",
  "description": "A chrome extension that saves bookmark space.",
  "version": "0.0.1",
  "default_locale": "en",
  "author": "AravNarula/Sean",
  "icons": {
    "16": "/img/s-note.png",
    
  },

  "browser_action": {
    "default_icon": "/img/icon64.png",
    "default_title": "PrainTab - A chrome extension that saves bookmark space.",
    "default_popup": "popup.html"
  },

  "permissions": ["activeTab", "tabs", "newtab"],

  "background": {
    "scripts": ["js/main.js"]
  },
    "chrome_url_overrides": {
    "newtab": "newtab.html"
  }
}
